
public class calculate {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		Calculator clt1 = new Calculator();
		
		
		System.out.println(clt1.performAddition(4, 8, 10));
		
		System.out.println(clt1.performAddition(4, 5));
		
		System.out.println(clt1.performAddition(3.4, 5.6));

	}

}
